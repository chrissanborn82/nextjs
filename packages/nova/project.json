{
  "sourceRoot": "packages/nova",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "packages/nova",
        "outputPath": "dist/packages/nova"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "nova:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "nova:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "nova:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "nova:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/nova"],
      "options": {
        "jestConfig": "packages/nova/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/nova/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
